{
	"name": "cadquery",
	"displayName": "CadQuery",
	"publisher": "roipoussiere",
	"description": "A preview panel for your CadQuery models.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Visualization"
	],
	"main": "./extension.js",
	"activationEvents": [
		"onCommand:cadquery.render"
	],
	"contributes": {
		"commands": [
			{
				"command": "cadquery.render",
				"title": "Open CadQuery viewer"
			},
			{
				"command": "cadquery.update",
				"title": "Update CadQuery model"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"three-cad-viewer": "^1.5.5",
		"xhr2": "^0.2.1"
	}
}
