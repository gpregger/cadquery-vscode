{
    "name": "cadquery",
	"displayName": "CadQuery",
	"publisher": "roipoussiere",
	"description": "A preview panel for your CadQuery models.",
	"version": "0.0.1",
	"engines": { "vscode": "^1.65.0" },
	"categories": [ "Visualization"	],
	"main": "./extension.js",
	"activationEvents": [ "onCommand:cadquery.render" ],
	"contributes": {
		"commands": [{
            "command": "cadquery.render",
            "title": "Open CadQuery viewer"
		}, {
            "command": "cadquery.config",
            "title": "Configure CadQuery viewer"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint"
	},
	"devDependencies": {
        "@types/vscode": "^1.65.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.3"
    }
}
