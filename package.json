{
	"name": "cadquery",
	"displayName": "CadQuery",
	"publisher": "roipoussiere",
	"description": "Create CadQuery 3D models within your IDE.",
	"keywords": [
		"cad",
		"cadquery",
		"python",
		"3d",
		"parametric",
		"modeling"
	],
	"homepage": "https://framagit.org/roipoussiere/cadquery-vscode",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://framagit.org/roipoussiere/cadquery-vscode.git"
	},
	"bugs": "https://framagit.org/roipoussiere/cadquery-vscode/-/issues",
	"license": "MIT",
	"author": "Roipoussere (https://mastodon.tetaneutral.net/@roipoussiere)",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Visualization"
	],
	"main": "./dist/main.js",
	"activationEvents": [
		"onCommand:cadquery.render"
	],
	"contributes": {
		"commands": [
			{
				"command": "cadquery.render",
				"title": "Open CadQuery viewer"
			},
			{
				"command": "cadquery.update",
				"title": "Update CadQuery model"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"vscode:prepublish": "yarn run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./extension.js --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "yarn run esbuild-base -- --sourcemap",
		"esbuild-watch": "yarn run esbuild-base -- --sourcemap --watch"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@vscode/test-electron": "^2.1.3",
		"esbuild": "^0.14.29",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"vsce": "^2.7.0"
	},
	"dependencies": {
		"three-cad-viewer": "^1.5.5",
		"xhr2": "^0.2.1"
	}
}
